<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
         xmlns:rng="http://relaxng.org/ns/structure/1.0"
         xmlns:x="http://purl.org/net/xml2rfc/ext"
         xmlns:xi="http://www.w3.org/2001/XInclude"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"><!-- xml2rfc Version 3 grammar -->
   <define name="rfc">
      <element name="rfc">
         <a:annotation>This is the root element of the xml2rfc vocabulary.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="number">
               <a:annotation>The number of the RFC to be produced.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="obsoletes" a:defaultValue="">
               <a:annotation>A comma-separated list of RFC numbers or Internet-Draft names.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="updates" a:defaultValue="">
               <a:annotation>A comma-separated list of RFC numbers or Internet-Draft names.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="category">
               <a:annotation>Document category (see Appendix "The "category" Attribute" of Internet-Draft draft-iab-rfc7991bis-05).</a:annotation>
               <choice>
                  <value>std</value>
                  <value>bcp</value>
                  <value>exp</value>
                  <value>info</value>
                  <value>historic</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="mode"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="consensus" a:defaultValue="false">
               <a:annotation>Affects the generated boilerplate. Note that the values of "no" and "yes" are deprecated and are replaced by "false" (the default) and "true".</a:annotation>
               <choice>
                  <value>no</value>
                  <value>yes</value>
                  <value>false</value>
                  <value>true</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="seriesNo">
               <a:annotation>Number within a document series.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="ipr">
               <a:annotation>Represents the Intellectual Property status of the document. See Appendix "The "ipr" Attribute" of Internet-Draft draft-iab-rfc7991bis-05 for details.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="iprExtract">
               <a:annotation>Identifies a single section within the document for which extraction "as is" is explicitly allowed (only relevant for historic values of the "ipr" attribute).</a:annotation>
               <data type="IDREF"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="submissionType" a:defaultValue="IETF">
               <a:annotation>The document stream, as described in [RFC7841]. (The RFC Series Editor may change the list of allowed values in the future.)</a:annotation>
               <choice>
                  <value>IETF</value>
                  <value>IAB</value>
                  <value>IRTF</value>
                  <value>independent</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="docName">
               <a:annotation>For Internet-Drafts, this specifies the draft name (which appears below the title).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="sortRefs" a:defaultValue="false">
               <a:annotation>Specifies whether or not the prep tool will sort the references in each reference section.</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="symRefs" a:defaultValue="true">
               <a:annotation>Specifies whether or not a formatter is requested to use symbolic references (such as "[RFC2119]"). If the value for this is "false", the references come out as numbers (such as "[3]").</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="tocInclude" a:defaultValue="true">
               <a:annotation>Specifies whether or not a formatter is requested to include a table of contents in generated files.</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="tocDepth" a:defaultValue="3">
               <a:annotation>Specifies the number of levels of headings that a formatter is requested to include in the table of contents; the default is "3".</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="prepTime">
               <a:annotation>The date that the XML was processed by a prep tool. This is included in the XML file just before it is saved to disk. The value is formatted using the "date-time" format defined in Section 5.6 of [RFC3339]. The "time-offset" should be "Z".</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="indexInclude" a:defaultValue="true">
               <a:annotation>Specifies whether or not a formatter is requested to include an index in generated files. If the source file has no &lt;iref&gt; elements, an index is never generated. This option is useful for generating documents where the source document has &lt;iref&gt; elements but the author no longer wants an index.</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="version">
               <a:annotation>Specifies the version of xml2rfc syntax used in this document. The only expected value (for now) is "3".</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="scripts" a:defaultValue="Common,Latin"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="expiresDate"><?hidden-in-prose?></attribute>
         </optional>
         <zeroOrMore>
            <ref name="link"/>
         </zeroOrMore>
         <ref name="front"/>
         <ref name="middle"/>
         <optional>
            <ref name="back"/>
         </optional>
      </element>
   </define>
   <define name="link">
      <element name="link">
         <a:annotation>A link to an external document that is related to the RFC.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <attribute name="href">
            <a:annotation>The URI of the external document.</a:annotation>
         </attribute>
         <optional>
            <attribute name="rel">
               <a:annotation>The relationship of the external document to this one. The relationships are taken from the "Link Relations" registry maintained by IANA [LINKRELATIONS].</a:annotation>
            </attribute>
         </optional>
      </element>
   </define>
   <define name="front">
      <element name="front">
         <a:annotation>Represents the "front matter": metadata (such as author information), the Abstract, and additional notes.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <ref name="title"/>
         <zeroOrMore>
            <ref name="seriesInfo"/>
         </zeroOrMore>
         <oneOrMore>
            <ref name="author"/>
         </oneOrMore>
         <optional>
            <ref name="date"/>
         </optional>
         <zeroOrMore>
            <ref name="area"/>
         </zeroOrMore>
         <zeroOrMore>
            <ref name="workgroup"/>
         </zeroOrMore>
         <zeroOrMore>
            <ref name="keyword"/>
         </zeroOrMore>
         <optional>
            <ref name="abstract"/>
         </optional>
         <zeroOrMore>
            <ref name="note"/>
         </zeroOrMore>
         <optional>
            <ref name="boilerplate"/>
         </optional>
         <optional>
            <ref name="toc"/>
         </optional>
      </element>
   </define>
   <define name="title">
      <element name="title">
         <a:annotation>Represents the document title.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="abbrev">
               <a:annotation>Specifies an abbreviated variant of the document title.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the title.</a:annotation>
            </attribute>
         </optional>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="br"/>
            </choice>
         </zeroOrMore>
      </element>
   </define>
   <define name="author">
      <element name="author">
         <a:annotation>Provides information about a document's author. This is used both for the document itself (at the beginning of the document) and for referenced documents.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for this &lt;author&gt; element.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="initials">
               <a:annotation>An abbreviated variant of the given name(s), to be used in conjunction with the separately specified surname. It usually appears on the front page, in footers, and in references.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="asciiInitials">
               <a:annotation>The ASCII equivalent of the author's initials, to be used in conjunction with the separately specified asciiSurname.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="surname">
               <a:annotation>The author's surname, to be used in conjunction with the separately specified initials. It usually appears on the front page, in footers, and in references.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="asciiSurname">
               <a:annotation>The ASCII equivalent of the author's surname, to be used in conjunction with the separately specified asciiInitials.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="fullname">
               <a:annotation>The full name (used in the automatically generated "Author's Address" section). Although this attribute is optional, if one or more of the "asciiFullname", "asciiInitials", or "asciiSurname" attributes have values, the "fullname" attribute is required.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="role">
               <a:annotation>Specifies the role the author had in creating the document.</a:annotation>
               <value>editor</value>
            </attribute>
         </optional>
         <optional>
            <attribute name="asciiFullname">
               <a:annotation>The ASCII equivalent of the author's full name.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <ref name="organization"/>
         </optional>
         <optional>
            <ref name="address"/>
         </optional>
      </element>
   </define>
   <define name="contact">
      <element name="contact">
         <a:annotation>Provides information about a contact, such as a contributor to be mentioned in an "Acknowledgements" section.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for this comment.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="initials">
               <a:annotation>See the corresponding attribute on &lt;author&gt; element (Section ""initials" Attribute" of Internet-Draft draft-iab-rfc7991bis-05).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="asciiInitials">
               <a:annotation>See the corresponding attribute on &lt;author&gt; element (Section ""asciiInitials" Attribute" of Internet-Draft draft-iab-rfc7991bis-05).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="surname">
               <a:annotation>See the corresponding attribute on &lt;author&gt; element (Section ""surname" Attribute" of Internet-Draft draft-iab-rfc7991bis-05).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="asciiSurname">
               <a:annotation>See the corresponding attribute on &lt;author&gt; element (Section ""asciiSurname" Attribute" of Internet-Draft draft-iab-rfc7991bis-05).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="fullname">
               <a:annotation>See the corresponding attribute on &lt;author&gt; element (Section ""fullname" Attribute" of Internet-Draft draft-iab-rfc7991bis-05).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="asciiFullname">
               <a:annotation>See the corresponding attribute on &lt;author&gt; element (Section ""asciiFullname" Attribute" of Internet-Draft draft-iab-rfc7991bis-05).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <ref name="organization"/>
         </optional>
         <optional>
            <ref name="address"/>
         </optional>
      </element>
   </define>
   <define name="organization">
      <element name="organization">
         <a:annotation>Specifies the affiliation [RFC7322] of an author.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="abbrev">
               <a:annotation>Abbreviated variant.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the organization's name.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="asciiAbbrev">
               <a:annotation>To support abbreviated organization names in both ASCII and non-ASCII contexts.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="showOnFrontPage" a:defaultValue="true">
               <a:annotation>To support turning off listing organization with author name.</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="address">
      <element name="address">
         <a:annotation>Provides address information for the author.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <ref name="postal"/>
         </optional>
         <optional>
            <ref name="phone"/>
         </optional>
         <optional>
            <ref name="facsimile"/>
         </optional>
         <zeroOrMore>
            <ref name="email"/>
         </zeroOrMore>
         <optional>
            <ref name="uri"/>
         </optional>
      </element>
   </define>
   <define name="postal">
      <element name="postal">
         <a:annotation>Contains optional child elements providing postal information. A postal address can contain only an ordered set of &lt;postalLine&gt; elements optionally followed by &lt;country&gt;, or only a set of &lt;street&gt;, &lt;city&gt;, &lt;region&gt;, &lt;code&gt;, and &lt;country&gt; elements, but not both.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <choice>
            <zeroOrMore>
               <choice>
                  <ref name="city"/>
                  <ref name="cityarea"/>
                  <ref name="code"/>
                  <ref name="country"/>
                  <ref name="extaddr"/>
                  <ref name="pobox"/>
                  <ref name="region"/>
                  <ref name="sortingcode"/>
                  <ref name="street"/>
               </choice>
            </zeroOrMore>
            <group>
               <oneOrMore>
                  <ref name="postalLine"/>
               </oneOrMore>
               <optional>
                  <ref name="country"/>
               </optional>
            </group>
         </choice>
      </element>
   </define>
   <define name="cityarea">
      <element name="cityarea">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the city area name.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="extaddr">
      <element name="extaddr">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>ASCII equivalent for extaddr.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="pobox">
      <element name="pobox">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>ASCII equivalent for pobox.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="sortingcode">
      <element name="sortingcode">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>ASCII equivalent for sortingcode.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="street">
      <element name="street">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the street address.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="city">
      <element name="city">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the city name.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="region">
      <element name="region">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the region name.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="code">
      <element name="code">
         <a:annotation>Deprecated, use &lt;postalLine&gt; instead.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the postal code.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="country">
      <element name="country">
         <a:annotation>Gives the country name or code in a postal address.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the country name.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="postalLine">
      <element name="postalLine">
         <a:annotation>Represents one line of a postal address. When more than one &lt;postalLine&gt; is given, the prep tool emits them in the order given.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the text in the address line.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="phone">
      <element name="phone">
         <a:annotation>Represents a phone number.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="facsimile">
      <element name="facsimile">
         <a:annotation>Deprecated. The &lt;email&gt; element is a much more useful way to get in touch with authors.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="email">
      <element name="email">
         <a:annotation>Provides an email address.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="ascii">
               <a:annotation>The ASCII equivalent of the author's email address. This is only used if the email address has any internationalized components.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="uri">
      <element name="uri">
         <a:annotation>Contains a web address associated with the author.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="date">
      <element name="date">
         <a:annotation>Provides information about the publication date. This element is used for two cases: the boilerplate of the document being produced, and inside bibliographic references that use the &lt;front&gt; element.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="day">
               <a:annotation>The day of publication.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="month">
               <a:annotation>The month or months of publication.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="year">
               <a:annotation>The year or years of publication.</a:annotation>
            </attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="area">
      <element name="area">
         <a:annotation>Provides information about the IETF area to which this document relates (currently not used when generating documents).</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="workgroup">
      <element name="workgroup">
         <a:annotation>This element is used to specify the Working Group (IETF) or Research Group (IRTF) from which the document originates, if any. The recommended format is the official name of the Working Group (with some capitalization).</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="keyword">
      <element name="keyword">
         <a:annotation>Specifies a keyword applicable to the document.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <text/>
      </element>
   </define>
   <define name="abstract">
      <element name="abstract">
         <a:annotation>Contains the Abstract of the document. See [RFC7322] for more information on restrictions for the Abstract.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for the Abstract.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <oneOrMore>
            <choice>
               <ref name="dl"/>
               <ref name="ol"/>
               <ref name="t"/>
               <ref name="ul"/>
            </choice>
         </oneOrMore>
      </element>
   </define>
   <define name="note">
      <element name="note">
         <a:annotation>Creates an unnumbered, titled block of text that appears after the Abstract.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="title">
               <a:annotation>Deprecated. Use &lt;name&gt; instead.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="removeInRFC" a:defaultValue="false">
               <a:annotation>If set to "true", this note is marked in the prep tool with text indicating that it should be removed before the document is published as an RFC. That text will be "This note is to be removed before publishing as an RFC."</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <ref name="name"/>
         </optional>
         <oneOrMore>
            <choice>
               <ref name="dl"/>
               <ref name="ol"/>
               <ref name="t"/>
               <ref name="ul"/>
            </choice>
         </oneOrMore>
      </element>
   </define>
   <define name="boilerplate">
      <element name="boilerplate">
         <a:annotation>Holds the boilerplate text for the document. This element is filled in by the prep tool.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <oneOrMore>
            <ref name="section"/>
         </oneOrMore>
      </element>
   </define>
   <define name="toc">
      <element name="toc">
         <a:annotation>This element contains the Table of Contents. It is created automatically by the preptool based on the "tocInclude" and "tocDepth" attributes of the &lt;rfc&gt; element and the section headers. In prepared drafts, it has no effect on document rendering and contains no useful information. In prepared RFCs, it is used as the source for the table of contents.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <zeroOrMore>
            <ref name="section"/>
         </zeroOrMore>
      </element>
   </define>
   <define name="middle">
      <element name="middle">
         <a:annotation>Represents the main content of the document.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <oneOrMore>
            <ref name="section"/>
         </oneOrMore>
      </element>
   </define>
   <define name="section">
      <element name="section">
         <a:annotation>Represents a section (when inside a &lt;middle&gt; element) or an appendix (when inside a &lt;back&gt; element).</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for this section.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="title">
               <a:annotation>Deprecated. Use &lt;name&gt; instead.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="numbered" a:defaultValue="true">
               <a:annotation>If set to "false", the formatter is requested to not display a section number. The prep tool will verify that such a section is not followed by a numbered section in this part of the document and will verify that the section is a top-level section. Descendant sections of unnumbered sections are unnumbered by definition.</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="toc" a:defaultValue="default">
               <a:annotation>Indicates to a formatter whether or not the section is to be included in a table of contents, if such a table of contents is produced. This only takes effect if the level of the section would have appeared in the table of contents based on the "tocDepth" attribute of the &lt;rfc&gt; element, and of course only if the table of contents is being created based on the "tocInclude" attribute of the &lt;rfc&gt; element. If this is set to "exclude", any section below this one will be excluded as well. The "default" value indicates inclusion of the section if it would be included by the tocDepth attribute of the &lt;rfc&gt; element.</a:annotation>
               <choice>
                  <value>include</value>
                  <value>exclude</value>
                  <value>default</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="removeInRFC" a:defaultValue="false">
               <a:annotation>If set to "true", this note is marked in the prep tool with text indicating that it should be removed before the document is published as an RFC. That text will be "This note is to be removed before publishing as an RFC."</a:annotation>
               <choice>
                  <value>true</value>
                  <value>false</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <ref name="name"/>
         </optional>
         <zeroOrMore>
            <choice>
               <ref name="artset"/>
               <ref name="artwork"/>
               <ref name="aside"/>
               <ref name="author"/>
               <ref name="blockquote"/>
               <ref name="contact"/>
               <ref name="dl"/>
               <ref name="figure"/>
               <ref name="iref"/>
               <ref name="ol"/>
               <ref name="sourcecode"/>
               <ref name="t"/>
               <ref name="table"/>
               <ref name="texttable"/>
               <ref name="ul"/>
            </choice>
         </zeroOrMore>
         <zeroOrMore>
            <ref name="section"/>
         </zeroOrMore>
      </element>
   </define>
   <define name="name">
      <element name="name">
         <a:annotation>The name of the section, note, figure, or texttable. This name can indicate markup of flowing text (for example, including references or making some characters use a fixed-width font).</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="slugifiedName"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="bcp14"/>
               <ref name="br"/>
               <ref name="cref"/>
               <ref name="em"/>
               <ref name="eref"/>
               <ref name="iref"/>
               <ref name="relref"/>
               <ref name="strong"/>
               <ref name="sub"/>
               <ref name="sup"/>
               <ref name="tt"/>
               <ref name="xref"/>
            </choice>
         </zeroOrMore>
      </element>
   </define>
   <define name="br">
      <element name="br">
         <a:annotation>Forces a line break. Since the layout and column widths of a document vary from one rendering to another, authors should use this element sparingly and consider its effect in all of the likely renderings. In some cases a U+200B, ZERO WIDTH SPACE character as a hint as a place where a block of text might be broken is a better choice.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="t">
      <element name="t">
         <a:annotation>Contains a paragraph of text.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for this paragraph.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="hangText">
               <a:annotation>Deprecated. Instead, use &lt;dd&gt; inside of a definition list (&lt;dl&gt;).</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="indent" a:defaultValue="0">
               <a:annotation>The indentation of the text element. A numeric value is interpreted as characters when rendering plain-text documents, and en-space units otherwise. Only non-negative integer indentation is allowed.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="keepWithNext" a:defaultValue="false">
               <a:annotation>Acts as a hint to the output formatters that do pagination to do a best-effort attempt to keep the paragraph with the next element, whatever that happens to be. For example, the HTML output @media print CSS ("CSS" refers to Cascading Style Sheets) might translate this to page-break-after: avoid. For PDF, the paginator could attempt to keep the paragraph with the next element. Note: this attribute is strictly a hint and not always actionable.</a:annotation>
               <choice>
                  <value>false</value>
                  <value>true</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="keepWithPrevious" a:defaultValue="false">
               <a:annotation>Acts as a hint to the output formatters that do pagination to do a best-effort attempt to keep the paragraph with the previous element, whatever that happens to be. For example, the HTML output @media print CSS might translate this to page-break-before: avoid. For PDF, the paginator could attempt to keep the paragraph with the previous element. Note: this attribute is strictly a hint and not always actionable.</a:annotation>
               <choice>
                  <value>false</value>
                  <value>true</value>
               </choice>
            </attribute>
         </optional>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="bcp14"/>
               <ref name="br"/>
               <ref name="contact"/>
               <ref name="cref"/>
               <ref name="em"/>
               <ref name="eref"/>
               <ref name="iref"/>
               <ref name="relref"/>
               <ref name="spanx"/>
               <ref name="strong"/>
               <ref name="sub"/>
               <ref name="sup"/>
               <ref name="tt"/>
               <ref name="u"/>
               <ref name="vspace"/>
               <ref name="xref"/>
            </choice>
         </zeroOrMore>
      </element>
   </define>
   <define name="aside">
      <element name="aside">
         <a:annotation>This element is a container for content that is semantically less important or tangential to the content that surrounds it.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for this aside.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <zeroOrMore>
            <choice>
               <ref name="artset"/>
               <ref name="artwork"/>
               <ref name="blockquote"/>
               <ref name="dl"/>
               <ref name="figure"/>
               <ref name="iref"/>
               <ref name="ol"/>
               <ref name="t"/>
               <ref name="table"/>
               <ref name="ul"/>
            </choice>
         </zeroOrMore>
      </element>
   </define>
   <define name="blockquote">
      <element name="blockquote">
         <a:annotation>Specifies that a block of text is a quotation.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for this quotation.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="cite">
               <a:annotation>The source of the citation. This must be a URI. If the "quotedFrom" attribute is given, this URI will be used by processing tools as the link for the text of that attribute.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="quotedFrom">
               <a:annotation>Name of person or document the text in this element is quoted from. A formatter should render this as visible text at the end of the quotation.</a:annotation>
            </attribute>
         </optional>
         <choice>
            <oneOrMore>
               <choice>
                  <ref name="artset"/>
                  <ref name="artwork"/>
                  <ref name="dl"/>
                  <ref name="figure"/>
                  <ref name="ol"/>
                  <ref name="sourcecode"/>
                  <ref name="t"/>
                  <ref name="ul"/>
               </choice>
            </oneOrMore>
            <oneOrMore>
               <choice>
                  <text/>
                  <ref name="bcp14"/>
                  <ref name="br"/>
                  <ref name="cref"/>
                  <ref name="em"/>
                  <ref name="eref"/>
                  <ref name="iref"/>
                  <ref name="relref"/>
                  <ref name="strong"/>
                  <ref name="sub"/>
                  <ref name="sup"/>
                  <ref name="tt"/>
                  <ref name="u"/>
                  <ref name="xref"/>
               </choice>
            </oneOrMore>
         </choice>
      </element>
   </define>
   <define name="list">
      <element name="list">
         <a:annotation>Deprecated. Instead, use &lt;dl&gt; for list/@style "hanging"; &lt;ul&gt; for list/@style "empty" or "symbols"; and &lt;ol&gt; for list/@style "letters", "numbers", "counter", or "format".</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="style" a:defaultValue="empty">
               <a:annotation>Deprecated.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="hangIndent">
               <a:annotation>Deprecated. Use &lt;dl&gt; instead.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="counter">
               <a:annotation>Deprecated. The functionality of this attribute has been replaced with &lt;ol&gt;/@start.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <oneOrMore>
            <ref name="t"/>
         </oneOrMore>
      </element>
   </define>
   <define name="ol">
      <element name="ol">
         <a:annotation>An ordered list. The labels on the items will be either a number or a letter, depending on the value of the type attribute.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for the list.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="type" a:defaultValue="1">
               <a:annotation>The type of the labels on list items. If the length of the type value is 1, the meaning is the same as it is for HTML:</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="start" a:defaultValue="1">
               <a:annotation>The ordinal value at which to start the list. This defaults to "1" and must be an integer of 0 or greater.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="group">
               <a:annotation>When the prep tool sees an &lt;ol&gt; element with a "group" attribute that has already been seen, it continues the numbering of the list from where the previous list with the same group name left off. If an &lt;ol&gt; element has both a "group" attribute and a "start" attribute, the group's numbering is reset to the given start value.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="spacing" a:defaultValue="normal">
               <a:annotation>Defines whether or not there is a blank line between entries. spacing="normal" indicates a single blank line, while spacing="compact" indicates no space between.</a:annotation>
               <choice>
                  <value>normal</value>
                  <value>compact</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="indent" a:defaultValue="adaptive">
               <a:annotation>The indentation of the list elements relative to the start of the list item number. With indent='adaptive', the widest list item number determines the indentation. A numeric value is interpreted as characters when rendering plain-text documents, and en-space units otherwise. Only non-negative integer indentation is allowed.</a:annotation>
               <choice>
                  <text/>
                  <value>adaptive</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <oneOrMore>
            <ref name="li"/>
         </oneOrMore>
      </element>
   </define>
   <define name="ul">
      <element name="ul">
         <a:annotation>An unordered list. The labels on the items will be symbols picked by the formatter.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for the list.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="spacing" a:defaultValue="normal">
               <a:annotation>Defines whether or not there is a blank line between entries. spacing="normal" indicates a single blank line, while spacing="compact" indicates no space between.</a:annotation>
               <choice>
                  <value>normal</value>
                  <value>compact</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="empty" a:defaultValue="false">
               <a:annotation>Defines whether or not the label is empty. empty="true" indicates that no label will be shown.</a:annotation>
               <choice>
                  <value>false</value>
                  <value>true</value>
               </choice>
            </attribute>
            <optional>
               <attribute name="bare" a:defaultValue="false">
                  <a:annotation>Can only be used with empty="true" (see below). This attribute controls whether the blank bullet has an horizontal extension or not. With bare="false", the empty list bullet will still occupy the same space as for empty="false". With empty="true", there will be no bullet at all, i.e., the list items will have no indentation.</a:annotation>
                  <choice>
                     <value>true</value>
                     <value>false</value>
                  </choice>
               </attribute>
            </optional>
         </optional>
         <optional>
            <attribute name="indent" a:defaultValue="3">
               <a:annotation>The indentation of the list elements relative to the start of the bullet or bullet text. A numeric value is interpreted as characters when rendering plain-text documents, and en-space units otherwise. Only non-negative integer indentation is allowed.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <oneOrMore>
            <ref name="li"/>
         </oneOrMore>
      </element>
   </define>
   <define name="li">
      <element name="li">
         <a:annotation>A list element, used in &lt;ol&gt; and &lt;ul&gt;.</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for this list item.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="derivedCounter"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <choice>
            <oneOrMore>
               <choice>
                  <ref name="artset"/>
                  <ref name="artwork"/>
                  <ref name="aside"/>
                  <ref name="blockquote"/>
                  <ref name="dl"/>
                  <ref name="figure"/>
                  <ref name="ol"/>
                  <ref name="sourcecode"/>
                  <ref name="t"/>
                  <ref name="table"/>
                  <ref name="ul"/>
               </choice>
            </oneOrMore>
            <oneOrMore>
               <choice>
                  <text/>
                  <ref name="bcp14"/>
                  <ref name="br"/>
                  <ref name="cref"/>
                  <ref name="em"/>
                  <ref name="eref"/>
                  <ref name="iref"/>
                  <ref name="relref"/>
                  <ref name="strong"/>
                  <ref name="sub"/>
                  <ref name="sup"/>
                  <ref name="tt"/>
                  <ref name="u"/>
                  <ref name="xref"/>
               </choice>
            </oneOrMore>
         </choice>
      </element>
   </define>
   <define name="dl">
      <element name="dl">
         <a:annotation>A definition list. Each entry has a pair of elements: a term (&lt;dt&gt;) and a definition (&lt;dd&gt;). (This is slightly different and simpler than the model used in HTML, which allows for multiple terms for a single definition.)</a:annotation>
         <optional>
            <attribute name="xml:base"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="xml:lang"><?hidden-in-prose?></attribute>
         </optional>
         <optional>
            <attribute name="anchor">
               <a:annotation>Document-wide unique identifier for the list.</a:annotation>
               <data type="ID"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="spacing" a:defaultValue="normal">
               <a:annotation>Defines whether or not there is a blank line between entries. spacing="normal" indicates a single blank line, while spacing="compact" indicates no space between.</a:annotation>
               <choice>
                  <value>normal</value>
                  <value>compact</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="newline" a:defaultValue="false">
               <a:annotation>The "newline" attribute defines whether or not the term appears on the same line as the definition. newline="false" indicates that the term is to the left of the definition, while newline="true" indicates that the term will be on a separate line.</a:annotation>
               <choice>
                  <value>false</value>
                  <value>true</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="indent" a:defaultValue="3">
               <a:annotation>Indicates the indentation to be used for the rendering of the second and following lines of the item (the first line starts with the term, and is not indented). The indentation amount is interpreted as characters when rendering plain-text documents, and en-space units when rendering in formats that have richer typographic support such as HTML or PDF. One en-space is assumed to be the length of 0.5 em-space in CSS units.</a:annotation>
            </attribute>
         </optional>
         <optional>
            <attribute name="pn"><?hidden-in-prose?>
               <data type="ID"/>
            </attribute>
         </optional>
         <oneOrMore>
            <ref name="dt"/>
            <ref name="dd"/>
         </oneOrMore>
      </element>
   </define>
   <define name="dt">
